{
  "productName": "enviro",
  "version": "0.0.0",
  "identifier": "com.tauri.dev",
  "build": {
    "beforeBuildCommand": "pnpm run next-build",
    "beforeDevCommand": "pnpm run next-dev",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../out"
  },
  "app": {
    "windows": [
      {
        "title": "",
        "width": 900,
        "height": 640,
        "titleBarStyle": "Overlay"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
