{
  "name": "enviro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tauri dev",
    "ui:add": "pnpm dlx shadcn-ui@latest add",
    "ui:diff": "pnpm dlx shadcn-ui@latest diff",
    "build:intel": "tauri build --target x86_64-apple-darwin",
    "build:silicon": "tauri build --target aarch64-apple-darwin",
    "build": "pnpm build:intel && pnpm build:silicon",
    "clean": "rimraf dist out node_modules .next && pnpm i",
    "next-dev": "next dev --turbo",
    "next-build": "next build",
    "next-start": "next start",
    "next-lint": "eslint ./ --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "tauri": "tauri"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@tauri-apps/api": ">=2.0.0-beta.0",
    "@tauri-apps/plugin-dialog": "2.0.0-beta.2",
    "@tauri-apps/plugin-fs": "2.0.0-beta.2",
    "@tauri-apps/plugin-shell": ">=2.0.0-beta.0",
    "@tauri-apps/plugin-store": "2.0.0-beta.2",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "lucide-react": "0.372.0",
    "next": "14.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-resizable-panels": "^2.0.18",
    "tailwind-merge": "2.3.0",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "2.0.0-beta.13",
    "@types/node": "20.12.2",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.23",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.2.2",
    "eslint-config-prettier": "9.1.0",
    "internal-ip": "8.0.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "rimraf": "5.0.5",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.3",
    "typescript-eslint": "7.7.0"
  }
}
